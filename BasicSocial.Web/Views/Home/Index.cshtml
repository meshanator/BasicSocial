@model BasicSocial.Core.User
@{
    ViewBag.Title = "Home Page";
}

<div>@Model.FirstName</div>
<div>@Model.LastName</div>
<div>@Model.Age</div>

<div class="ui list">
    @foreach (var post in Model.ReceivedPosts)
    {
        <div class="item">@post.Name</div>
    }
</div>

<div class="ui list">
    @foreach (var post in Model.Friends)
    {
        <div class="item">@post.UserName</div>
    }
</div>
